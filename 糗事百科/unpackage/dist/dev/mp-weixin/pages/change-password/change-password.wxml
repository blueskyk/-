<view class="change-password"><input class="uni-input common-input" type="password" placeholder="输入旧密码" data-event-opts="{{[['input',[['__set_model',['','oldPassWord','$event',[]]]]]]}}" value="{{oldPassWord}}" bindinput="__e"/><input class="uni-input common-input" type="password" placeholder="输入新密码" data-event-opts="{{[['input',[['__set_model',['','newPassWord','$event',[]]]]]]}}" value="{{newPassWord}}" bindinput="__e"/><input class="uni-input common-input" type="password" placeholder="输入确认密码" data-event-opts="{{[['input',[['__set_model',['','rePassWord','$event',[]]]]]]}}" value="{{rePassWord}}" bindinput="__e"/><button class="{{['user-set-btn',(disabled)?'user-set-btn-disable':'']}}" type="default" loading="{{loading}}" disabled="{{disabled}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">完成</button></view>