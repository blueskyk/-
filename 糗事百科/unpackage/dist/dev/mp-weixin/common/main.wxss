.uni-card-header {
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	min-height: 50rpx;
	padding: 20rpx 30rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
